		<type name="sif:setor">
			<parent>dl:dataListItem</parent>
			<properties>
				<property name="sif:codigo_setor">
					<type>d:text</type>
					<mandatory>true</mandatory>
				</property>
				<property name="sif:sigla_setor">
					<type>d:text</type>
					<mandatory>true</mandatory>
				</property>
			</properties>
			<associations>
				<association name="sif:setor_superior">
					<source>
						<mandatory>false</mandatory>
						<many>true</many>
					</source>
					<target>
						<class>sif:setor</class>
						<mandatory>false</mandatory>
						<many>true</many>
					</target>
				</association>
			</associations>
